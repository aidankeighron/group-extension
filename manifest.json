{
    "name": "Group Management",
    "permissions": [
        "tabs",
        "tabGroups",
        "storage"
    ],
    "icons": {
        "128": "images/image128.png",
        "48": "images/image48.png",
        "32": "images/image32.png",
        "24": "images/image24.png",
        "16": "images/image16.png"
    },
    "version": "3.1",
    "description": "Manage the saving and loading of tab groups",
    "manifest_version": 3,
    "author": "Aidan Keighron",
    "action":{
        "default_popup": "index.html",
        "default_title": "Group Management"
    }
}